<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<module>
	<!-- Inherit the core Web Toolkit stuff. -->
	<inherits name="com.google.common.cache.Cache" />
	<inherits name="com.google.common.collect.Collect" />
	<inherits name="com.google.gwt.http.HTTP" />
	<inherits name="com.google.gwt.i18n.I18N" />
	<inherits name="com.google.gwt.inject.Inject" />
	<inherits name="com.google.gwt.logging.Logging" />
	<inherits name="com.google.gwt.user.cellview.CellView" />
	<inherits name="com.google.gwt.user.User" />
	<inherits name="com.google.gwt.visualization.Visualization" />
	<inherits name="javax.annotation.Annotation" />
	<inherits name="name.pehl.piriti.json.JSON" />
	<inherits name="net.zschech.gwt.websockets.WebSockets" />
	<inherits name='com.allen_sauer.gwt.dnd.gwt-dnd' />
	<inherits name='com.google.api.gwt.services.Plus' />
	<inherits name='com.google.gwt.widgetideas.GWTCanvas' />

	<!-- Specify the app entry point class. -->
	<entry-point class="tv.dyndns.kishibe.qmaclone.client.QMAClone" />

	<inherits name="com.google.gwt.user.theme.standard.Standard" />
	<!-- <inherits name="com.google.gwt.user.theme.chrome.Chrome"/> -->
	<!-- <inherits name="com.google.gwt.user.theme.dark.Dark"/> -->

	<servlet class="tv.dyndns.kishibe.qmaclone.server.ServiceServlet" path="/service" />
	<servlet class="tv.dyndns.kishibe.qmaclone.server.IconUploadServlet" path="/icon" />
	<servlet class="tv.dyndns.kishibe.qmaclone.server.RssServlet" path="/rss.xml" />
	<servlet class="tv.dyndns.kishibe.qmaclone.server.ImageProxyServlet" path="/image" />
	<servlet class="tv.dyndns.kishibe.qmaclone.server.service.LinkServlet" path="/link" />
	<servlet class="tv.dyndns.kishibe.qmaclone.server.StatsServlet" path="/stats" />
	<servlet class="tv.dyndns.kishibe.qmaclone.server.RemoteLoggingServlet" path="/remote_logging" />

	<set-property name="compiler.emulatedStack" value="true" />
	<set-configuration-property name="compiler.emulatedStack.recordLineNumbers"
		value="true" />
	<set-configuration-property name="compiler.emulatedStack.recordFileNames"
		value="true" />
	<!-- Enabled by Controller.java -->
	<!-- <set-property name="gwt.logging.simpleRemoteHandler" value="ENABLED"
		/> -->
	<set-property name="gwt.logging.popupHandler" value="DISABLED" />

	<add-linker name="xsiframe"/>
	<set-configuration-property name="devModeRedirectEnabled" value="true"/>
	<set-property name="compiler.useSourceMaps" value="true" />
	<set-configuration-property name='xsiframe.failIfScriptTag' value='FALSE'/>
</module>
